pipeline {
  agent {
  label 'practice01'
};
  stages {
      stage ('Build') {
        steps {
          echo "This is Build stage"
          sh 'sleep 5'
        }
      }
    stage ('deploy') {
 parallel {
        steps {
          echo "This is deploy stage"
          sh 'sleep 5'
        }
      }
stage ('test') {
        steps {
          echo "This is test stage"
          sh 'sleep 5'
        }
      }
}
   }
}
